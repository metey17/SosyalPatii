/* preloader2 */
.preloader2 {

  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  z-index: 9999;
}
/* ... */
.loader2 { 
  position: absolute;
  left: 50%;
  top: 60%;
  transform: translate(-50%, -50%);
}
.loader2>div {
  width: 16px;
  height: 16px;
  margin: 0 4px;
  background-color: #FF4D84;
  display: inline-block;
  border-radius: 100%;
  -webkit-animation: pulse 1.2s infinite ease-in-out both;
  animation: loader2-pulse 1.2s infinite ease-in-out both;
}

.loader2 div:nth-child(2) {
  -webkit-animation-delay: -0.16s;
  animation-delay: -0.16s;
}

@-webkit-keyframes loader2-pulse {

  0%,
  80%,
  100% {
    -webkit-transform: scale(0)
  }

  40% {
    -webkit-transform: scale(1.0)
  }
}

@keyframes loader2-pulse {

  0%,
  80%,
  100% {
    -webkit-transform: scale(0);
    transform: scale(0);
  }

  40% {
    -webkit-transform: scale(1.0);
    transform: scale(1.0);
  }
}


